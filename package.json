{
  "name": "@doriongilmore/codingame-ts-engine",
  "version": "1.0.6",
  "description": "A set of tools to work on codingame projects within any IDE",
  "type": "module",
  "keywords": ["codingame", "engine", "typescript"],
  "scripts": {
    "pre_dev": "cp ./template.package.json ${CG_PATH}/package.json && npm --prefix ${CG_PATH} install",
    "_dev": "npm --prefix ${CG_PATH} run dev",
    "puzzle": "CG_PATH=./template/puzzle npm run _dev",
    "build": "tsc",
    "test": "echo \"Error: no test specified\""
  },
  "bin": {
    "buildForCodingame.ts": "bin/buildForCodingame.ts"
  },
  "exports": {
    "./types": "./src/types.ts",
    "./puzzle/*": "./src/Puzzle/*.ts",
    "./puzzle/*.js": "./src/Puzzle/*.ts"
  },
  "author": "doriongilmore<doriongilmore@outlook.com>",
  "license": "MIT",
  "dependencies": {
    "@types/node": "^22.12.0",
    "process.argv": "^1.0.0",
    "tsx": "^4.20.4",
    "typescript": "^5.9.2"
  }
}
